const images = Object.values(
  import.meta.glob(
    '../assets/icons/abilities/tanks/war/*.{png,jpg,jpeg,PNG,JPEG}',
    { eager: true, query: 'url' }
  )
);

const bloodwhetting = {
  name: 'BW',
  img: images[0].default,
};
const damnation = {
  name: 'Damnation',
  img: images[1].default,
};
const equilibrium = {
  name: 'Equilibrium',
  img: images[2].default,
};
const holmgang = {
  name: 'Holmgang',
  img: images[3].default,
};
const nascentflash = {
  name: 'Nascent',
  img: images[4].default,
};
const thrillofbattle = {
  name: 'Thrill',
  img: images[5].default,
};
const vengeance = {
  name: 'Vengeance',
  img: images[6].default,
};

const warMap = {
  Bloodwhetting: bloodwhetting,
  Damnation: damnation,
  Equilibrium: equilibrium,
  Holmgang: holmgang,
  'Nascent Buddy': nascentflash,
  Nascent: nascentflash,
  Thrill: thrillofbattle,
  vengeance,
  Vengeance: vengeance,
};
